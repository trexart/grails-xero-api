<plugin name='xero-api' version='0.1' grailsVersion='2.2.0 &gt; *'>
  <author>Stephanie Sherriff</author>
  <authorEmail>stephanie@trexart.com.au</authorEmail>
  <title>Grails Xero Api Plugin</title>
  <description>Grails plugin to simplify connecting to the Xero API.
</description>
  <documentation>http://grails.org/plugin/xero-api</documentation>
  <type>XeroApiGrailsPlugin</type>
  <resources>
    <resource>org.grails.plugins.xeroapi.OauthServiceException</resource>
    <resource>org.grails.plugins.xeroapi.XeroBadRequestException</resource>
    <resource>org.grails.plugins.xeroapi.XeroContactService</resource>
    <resource>org.grails.plugins.xeroapi.XeroException</resource>
    <resource>org.grails.plugins.xeroapi.XeroInvoiceService</resource>
    <resource>org.grails.plugins.xeroapi.XeroOrganisationService</resource>
    <resource>org.grails.plugins.xeroapi.XeroUnauthorizedException</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies>
    <test>
      <dependency group='net.sourceforge.cobertura' name='cobertura' version='1.9.4.1' />
    </test>
    <compile>
      <dependency group='org.freemarker' name='freemarker' version='2.3.16' />
      <dependency group='org.scribe' name='scribe' version='1.3.2' />
    </compile>
    <runtime>
      <dependency group='mysql' name='mysql-connector-java' version='5.1.20' />
    </runtime>
  </dependencies>
  <plugins>
    <test>
      <plugin group='org.grails.plugins' name='code-coverage' version='1.2.5' />
    </test>
    <compile>
      <plugin group='org.grails.plugins' name='cache' version='1.0.1' />
      <plugin group='org.grails.plugins' name='quartz2' version='2.1.6.2' />
    </compile>
    <runtime>
      <plugin group='org.grails.plugins' name='cloud-bees' version='0.6.2' />
      <plugin group='org.grails.plugins' name='resources' version='1.1.6' />
      <plugin group='org.grails.plugins' name='mail' version='1.0.1' />
      <plugin group='org.grails.plugins' name='fields' version='1.3' />
      <plugin group='org.grails.plugins' name='hibernate' version='2.2.0' />
      <plugin group='org.grails.plugins' name='database-migration' version='1.3.2' />
      <plugin group='org.grails.plugins' name='jquery' version='1.8.3' />
      <plugin group='org.grails.plugins' name='multi-tenant-single-db' version='0.8.2' />
      <plugin group='org.grails.plugins' name='twitter-bootstrap' version='2.3.0' />
      <plugin group='org.grails.plugins' name='spring-security-core' version='1.2.7.3' />
      <plugin group='org.grails.plugins' name='newrelic' version='0.1' />
    </runtime>
    <build>
      <plugin group='org.grails.plugins' name='tomcat' version='2.2.0' />
    </build>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>